/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@layer base {
  h1 {
    @apply text-3xl;
    @apply font-medium;
  }
  h2 {
    @apply text-2xl;
    @apply font-medium;
  }
  h3 {
    @apply text-lg;
  }
  a {
    @apply text-blue-600 underline;
  }
}
@tailwind components;
@tailwind utilities;

